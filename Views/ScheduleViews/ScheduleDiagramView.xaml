<Window x:Class="KKOK.Views.ScheduleViews.ScheduleDiagramView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KKOK.ViewModels.ViewModel.ScheduleViewModels"
        xmlns:view="clr-namespace:KKOK.Views.ScheduleViews"
        mc:Ignorable="d"
        Title="ScheduleDiagramView" Height="450" Width="800">
    <Window.DataContext>
        <local:ScheduleDiagramViewModel/>
    </Window.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="237*"/>
            <RowDefinition Height="197*"/>
        </Grid.RowDefinitions>
        <view:NoticeListView Grid.Row="0" Margin="50,0,50,5" DataContext="{Binding NoticeListViewModel}" />
        <view:ProjectProgressView Grid.Row="1" Margin="50,0,50,5" DataContext="{Binding ProjectProgressViewModel}" />
    </Grid>
</Window>
